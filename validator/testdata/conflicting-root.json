{
  "$schema": "https://www.designtokens.org/schemas/2025.10.json",
  "color": {
    "$type": "color",
    "$root": {
      "$value": {
        "colorSpace": "srgb",
        "components": [1, 0.42, 0.21],
        "alpha": 1
      },
      "$description": "Root token using $root"
    },
    "_": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0, 0.5, 1],
        "alpha": 1
      },
      "$description": "Group marker token (conflicting with $root)"
    }
  }
}
